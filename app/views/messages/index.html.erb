<h1>Hellow</h1>

<% @messages.each do |message| %>
    <h3> <%= message.title %> </h3>
    <h5> <%= message.description %> </h5>
    <%= link_to "Message" , message_path(message), class: "btn btn-default" %>
<% end %>
<button class="btn btn-success"><%= link_to "New message" , new_message_path %></button>